{
    "name": "namdp06/laravel-scout-aws-elasticsearch",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "aws", "elastic"],
    "require": {
        "php": ">=7.1",
        "laravel/scout": ">=6.0",
        "elasticsearch/elasticsearch": "~6.0",
        "jsq/amazon-es-php": ">=0.2.3"
    },
     "require-dev": {
        "fzaninotto/faker": ">=1.8",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\Test\\": "tests"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (>=6.0)."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
